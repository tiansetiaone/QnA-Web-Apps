{
  "version": 2,
  "builds": [
    {
      "src": "qna-backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "qna-frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/qna-backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/qna-frontend/$1"
    }
  ]
}
